<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
        
        <context:annotation-config></context:annotation-config>
        <bean id="p" class="com.cg.spring.trial.Patient" init-method="init" destroy-method="destroyBean">
        <constructor-arg name="patientId" value="123"></constructor-arg>
        <constructor-arg name="patientName" value="leela"></constructor-arg>
        <constructor-arg name="problem" value="nothing just checkup"></constructor-arg>
        </bean>
</beans>